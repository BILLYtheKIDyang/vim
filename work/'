use Data::Dumper;
print 'perl> ';
while (defined($s=<>)) {
   if (length($s) >= 2) {
      $code = $code . $s;
      print '..... ';
   } else {

      $result = eval $code;
      if ($@) {
         print "Invalid string: \n $code";
      } else {
         print Dumper($result);
      }
      $code = '';
      print 'perl> ';
   }
}
